<div class="group">
  <%= link_to("New record", new_admin_record_path, class: "btn btn-primary pull-right") %>
</div>
<table class="table table-hover">
<thead>
  <tr>
    <td>id</td>
    <td>項目</td>
    <td>距離</td>
    <td>性質</td>
    <td>分</td>
    <td>秒</td>
    <td>上傳者</td>
    <td>保持者</td>
  </tr>
</thead>
  <% @records.each do |record| %>
<tr>
  <td><%= record.id  %></td>
  <td><%= record.program_id  %></td>
  <td><%= record.distance  %></td>
  <td><%= record.property_id  %></td>
  <td><%= record.minutes  %></td>
  <td><%= record.seconds  %></td>
  <td><%= record.user_id  %></td>
  <td><%= record.owner_id  %></td>
</tr>

  <% end %>
</table>
